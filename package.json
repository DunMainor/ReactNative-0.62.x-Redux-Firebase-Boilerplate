{
  "name": "RNBoilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest",
    "lint": "npx eslint . --fix",
    "lint-watch": "esw -w --changed src/**",
    "clear": "jest --clearCache",
    "start": "npx react-native start --reset-cache",
    "build:android:release": "yarn clean:android && npx react-native run-android --variant=release",
    "build:android:debug": "npx react-native run-android --variant=debug",
    "build:ios:release": "yarn clean:ios && npx react-native run-ios --variant=release",
    "build:ios:debug": "npx react-native run-ios --variant=debug",
    "reactotron:reverse": "adb reverse tcp:9090 tcp:9090",
    "clean:android": "watchman watch-del-all && rm -rf /tmp/metro-bundler-cache-* && cd android && ./gradlew clean",
    "clean:ios": "cd ios && pod deintegrate && pod install",
    "prepare:android": "yarn clean:android && yarn reactotron:reverse && yarn start",
    "prepare:ios": "yarn clean:ios && yarn start",
    "bundle:android:release": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.9.0",
    "@react-native-community/masked-view": "^0.1.7",
    "@react-native-firebase/analytics": "^6.4.0",
    "@react-native-firebase/app": "^6.4.0",
    "@react-native-firebase/crashlytics": "^6.4.0",
    "@react-native-firebase/database": "^6.4.0",
    "@react-native-firebase/firestore": "^6.4.0",
    "@react-native-firebase/perf": "^6.4.0",
    "@react-navigation/bottom-tabs": "^5.2.5",
    "@react-navigation/drawer": "^5.4.0",
    "@react-navigation/native": "^5.1.4",
    "@react-navigation/stack": "^5.2.9",
    "axios": "^0.21.1",
    "md5": "^2.2.1",
    "native-base": "^2.13.12",
    "react": "16.11.0",
    "react-native": "0.62.3",
    "react-native-awesome-alerts": "^1.3.1",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-keychain": "^5.0.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal-overlay": "^1.3.1",
    "react-native-push-notification": "^3.1.9",
    "react-native-reanimated": "^1.7.1",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.4.0",
    "react-native-sensitive-info": "^5.5.5",
    "react-native-vector-icons": "^6.6.0",
    "react-redux": "^7.2.0",
    "reactotron-react-native": "^5.0.0",
    "reactotron-redux": "^3.1.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-persist-sensitive-storage": "^1.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@react-native-community/eslint-config": "^1.0.0",
    "babel-jest": "^25.2.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react-native": "^3.8.1",
    "jest": "^25.2.7",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
